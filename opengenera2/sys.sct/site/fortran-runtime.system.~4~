;;; SCRC's system site file for FORTRAN-77 Runtime system

(MULTIPLE-VALUE-BIND (MAJOR-VERSION MINOR-VERSION)
    (SI:GET-SYSTEM-VERSION)
  (IF (OR (< MAJOR-VERSION 369) (AND (= MAJOR-VERSION 369) (< MINOR-VERSION 507)))
      (SI:SET-SYSTEM-SOURCE-FILE "FORTRAN-RUNTIME" "SYS: FORTRAN; FORTRAN-RUNTIME")
      (SCT:SET-SYSTEM-DIRECTORY-FILE "FORTRAN-RUNTIME" "SYS: FORTRAN; FORTRAN-RUNTIME")))
