;; gate-array-support system, to be loaded on top of schematic-NS

(defsystem gate-array-NS
    (:pretty-name "Gate-Array-NS"
     :default-pathname "ns: Gate-Array-NS;"
     :journal-directory "ns: Gate-Array-NS;"
     :bug-reports (:mailing-list "Bug-Gate-Array-NS"
		   :documentation "Report a problem with Gate-Array-NS")
     :patchable t
     :initial-status :released
     :maintaining-sites (:scrc :s4cc :sww)
     :distribute-sources nil
     :distribute-binaries t)
  (:module schematic-ns schematic-ns (:type :system))
  (:serial
    schematic-ns 
    "gate-array-NS"
    "count-gate-array"
    "statistical-wire-capacitance"
    "gate-array-technologies"))
