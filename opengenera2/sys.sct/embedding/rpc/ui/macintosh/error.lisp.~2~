;;; This is the Lisp version of a portion of the file SYS:EMBEDDING;RPC;GENERIC;ERROR.H

;;; Error code definitions for all Symbolics RPC-based facilities.
;;; This could be split into separate files for each facility later,
;;; if it gets too large, but I think that is unlikely

;;; Error codes for the remote console facility

(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 10	;application_not_found_error
				"Application object not found")
(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 11	;screen_not_found_error
				"Screen not found")
(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 12	;window_not_found_error
				"Window not found")
(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 13	;window_already_exists_error
				"Window ID is already in use")

(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 20	;wrong_type_argument_error
				"Wrong type argument")
(RPC:DEFINE-REMOTE-ERROR-NUMBER :SYMBOLICS 21	;wrong_shape_bitmap_error
				"Bitmap argument is the wrong shape")
