(defun write-file-section-to-stream (encached-file version section stream)
  (loop for record being the text-records
	    in-version version			;get attributes for parent version
	    only-section section
	    of encached-file
	do (send stream :line-out record)	1;:line-out dosen't copy
0	   ))
