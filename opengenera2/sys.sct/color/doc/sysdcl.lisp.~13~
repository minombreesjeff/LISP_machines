(sage:define-documentation-system Color-Doc
    (:pretty-name "Color System Documentation"
     :default-pathname "SYS:COLOR;DOC;"
     :patchable t
     :advertised-in (:herald)
     :maintaining-sites :scrc
     :bug-reports (:name "Color System Documentation" :documentation
			 "Report problems with Color System documentation")
     ;; .SAB files are considered to be product files by the distribution dumper
     :distribute-sources t
     :distribute-binaries t
     :source-category :basic)

  (:module SGD-book-design SGD-book-design (:type :system))
  (:module defbook "defbook.lisp" (:type :lisp-read-only))
;  (:module ivory-color-doc ivory-color-doc (:type :system))
  (:serial
;;;    SGD-book-design
    defbook
    (:parallel
      "color-ch0-script"
      "color-ch01-introduction"
      "color-ch1-getting-started"
      "color-ch2-using-color-windows"
      "color-ch3-drawing-on-color-screens"
      "color-ch4-specifying-colors"
      "color-ch5-creating-color-maps"
      "color-ch6-using-color-hardware"
      "color-ch7-editing-colors-interactively"
      "color-ch8-running-the-color-demo"
      "color-ch9-in-case-of-trouble"
      "color-ch10-tutorial"
      "color-ch99-orphan-records"
      "color-ch11-lisp-dictionary"
      "color-ch11a-messages-1-e"
      "color-ch11b-messages-f-i"
      "color-ch11c-messages-j-m"
      "color-ch11d-messages-n-r"
      "color-ch11e-messages-s-only"
      "color-ch11f-messages-t-z"
      "color-chx-iman-functions-alphabetically"
      "color-chx-appendix-a-ivory-color"
      "color-chx-appendix-b-glossary")))

