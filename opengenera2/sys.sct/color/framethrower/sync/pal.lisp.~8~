(define-sync-program pal-601-640-by-576-sync photon
  :horizontal-frequency 15.625e3
  :interlaced? t
  :total-lines 625
  :visible-lines 576
  :visible-pixels-per-line 640
  :horizontal-blanking-time (* 144 (/ 1.0 13.5e6))
  :horizontal-sync-time 4.7e-6
  :horizontal-front-porch-time (* 12 (/ 1.0 13.5e6))
  :equalization-pulse-time 2.35e-6
  :serration-pulse-time 4.7e-6
  :vertical-sync-lines 2.5
  :vertical-front-porch-lines 2.5
  :pixel-clock-divisor 2
  :formats (:pal-601-640-by-576)
  :type :pal
  )						; Move to color with same name

