(define-sync-program pal-601-720-by-576-sync ntsc/pal-avp
  :pixel-frequency 13.5e6
  :interlaced? t
  :total-lines 625
  :visible-lines 576
  :visible-pixels-per-line 720
  :horizontal-sync-time 4.7e-6
  :equalization-pulse-time 2.35e-6
  :serration-pulse-time 4.7e-6
  :horizontal-blanking-pixels 144
  :horizontal-front-porch-pixels 12
  :vertical-sync-lines 2.5
  :vertical-front-porch-lines 2.5
  :formats (:pal-601-720-by-576)
  :type :pal
  )

(define-sync-program pal-640-by-576-sync ntsc/pal-avp
  :horizontal-frequency 15.625e3
  :interlaced? t
  :total-lines 625
  :visible-lines 576
  :visible-pixels-per-line 640
  :horizontal-blanking-time 12.05e-6
  :horizontal-sync-time 4.7e-6
  :horizontal-front-porch-time 1.65e-6
  :equalization-pulse-time 2.35e-6
  :serration-pulse-time 4.7e-6
  :vertical-sync-lines 2.5
  :vertical-front-porch-lines 2.5
  :formats (:pal-640-by-576)
  :type :pal
  )

(define-sync-program pal-720-by-576-sync ntsc/pal-avp
  :horizontal-frequency 15.625e3
  :interlaced? t
  :total-lines 625
  :visible-lines 576
  :visible-pixels-per-line 720
  :horizontal-blanking-time 12.05e-6
  :horizontal-sync-time 4.7e-6
  :horizontal-front-porch-time 1.65e-6
  :equalization-pulse-time 2.35e-6
  :serration-pulse-time 4.7e-6
  :vertical-sync-lines 2.5
  :vertical-front-porch-lines 2.5
  :formats (:pal :pal-720-by-576)
  :type :pal
  )

